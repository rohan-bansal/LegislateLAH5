y.", "pending_committee_id": 254, "committee": {"committee_id": 254, "chamber": "S", "chamber_id": 36, "name": "Assignments"}, "referrals": [{"date": "2015-08-04", "committee_id": 254, "chamber": "S", "chamber_id": 36, "name": "Assignments"}, {"date": "2016-01-27", "committee_id": 277, "chamber": "S", "chamber_id": 36, "name": "Judiciary"}, {"date": "2016-05-31", "committee_id": 254, "chamber": "S", "chamber_id": 36, "name": "Assignments"}], "history": [{"date": "2015-08-04", "action": "Filed with Secretary by Sen. Ira I. Silverstein", "chamber": "S", "chamber_id": 36, "importance": 1}, {"date": "2015-08-04", "action": "First Reading", "chamber": "S", "chamber_id": 36, "importance": 0}, {"date": "2015-08-04", "action": "Referred to Assignments", "chamber": "S", "chamber_id": 36, "importance": 1}, {"date": "2016-01-27", "action": "Assigned to Judiciary", "chamber": "S", "chamber_id": 36, "importance": 1}, {"date": "2016-02-17", "action": "To Subcommittee on Firearms (JU)", "chamber": "S", "chamber_id": 36, "importance": 0}, {"date": "2016-03-24", "action": "Added as Chief Co-Sponsor Sen. Jacqueline Y. Collins", "chamber": "S", "chamber_id": 36, "importance": 0}, {"date": "2016-04-08", "action": "Rule 2-10 Committee Deadline Established As April 22, 2016", "chamber": "S", "chamber_id": 36, "importance": 0}, {"date": "2016-04-22", "action": "Rule 2-10 Committee/3rd Reading Deadline Established As May 13, 2016", "chamber": "S", "chamber_id": 36, "importance": 0}, {"date": "2016-05-13", "action": "Rule 2-10 Committee/3rd Reading Deadline Established As May 27, 2016", "chamber": "S", "chamber_id": 36, "importance": 0}, {"date": "2016-05-27", "action": "Rule 2-10 Committee/3rd Reading Deadline Established As May 31, 2016", "chamber": "S", "chamber_id": 36, "importance": 0}, {"date": "2016-05-31", "action": "Rule 3-9(a) / Re-referred to Assignments", "chamber": "S", "chamber_id": 36, "importance": 1}], "sponsors": [{"people_id": 1074, "person_hash": "kqj2qj38", "party_id": 